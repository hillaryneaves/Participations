<Window x:Class="WPF_Web_Browser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Web_Browser"
        mc:Ignorable="d"
        Title="Let me google that for you!" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnGo" Content="Go" HorizontalAlignment="Center" Margin="0,259,0,0" VerticalAlignment="Top" Width="191" Click="btnGo_Click" IsDefault="True" Height="77"/>
        <TextBox x:Name="txtURL" HorizontalAlignment="Center" Margin="0,121,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="236" Height="81"/>

    </Grid>
</Window>
